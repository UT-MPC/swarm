{
    "dataset": "mnist",
    "mobility-model": "levy-walk",
    "client-num": 45,
    "local-set-size": 2,
    "local-data-size": 160,
    "goal-set-size": 5,
    "local-data-points": 120,
    "noise-percentage": 50,
    "district-9": [[1,2], [3,4], [5,6],
                    [7,8], [9,0], [1,2],
                    [4,5], [6,7], [8,9]],
    "strategies": {
        "local": true,
        "federated": true,
        "greedy": true,
        "opportunistic": true,
