{
    "dataset": "mnist",
    "intervals": [50, 50, 50],
    "local-set": [0,1],
    "goal-set": [0,1,2,3,4],
    "label-sets": [[2,3],[3,4,5],[4,5,6]],
    "number-of-data-points": 180,
    "noise-percentage": 50,
    "strategies": {
        "momentum": true,
        "local": true,
        "greedy-sim": true,
        "greedy-no-sim": true
    },
    "pretrained-model": "../pretrained/test_base_2nn_mnist.pickle",
    "initial-learning-rate": 0.001,
    "kappa": 7,
    "offset": 0.9,
    "apply_rate": 1
}